{{#subPages}}
    <div role="listitem" class="atlas-nav__item {{#type}}atlas-nav__item_{{type}}{{/type}}">
        <a class="atlas-nav__ln {{#type}}_{{type}}{{/type}} {{#noDocumentation}}_empty{{/noDocumentation}} {{#isDeprecated}}_deprecated{{/isDeprecated}} js-atlas-nav-ln" {{#id}}href="{{id}}.html" id="nav-{{id}}"{{/id}}
            data-name="{{title}}">
            {{#icon}}<svg width="14" height="14" viewBox="0 0 14 14" role="img"><use xlink:href="#icon-{{{icon}}}"></use></svg>{{/icon}}
            {{title}}
        </a>
        {{#subPages.length}}
            <div role="list" class="atlas-nav__sub-nav">
                {{>navigation}}
            </div>
        {{/subPages.length}}
    </div>
{{/subPages}}
